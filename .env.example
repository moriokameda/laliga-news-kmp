# Environment Variables for La Liga News App
# Copy this file to .env and fill in the actual values

# Android Signing Configuration
KEYSTORE_FILE=keystore/laliga-news-release.keystore
KEYSTORE_PASSWORD=your_keystore_password_here
KEY_ALIAS=laliga-news
KEY_PASSWORD=your_key_password_here

# API Keys (for future implementation)
NEWS_API_KEY=your_news_api_key_here
TRANSLATOR_API_KEY=your_microsoft_translator_key_here
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Firebase Configuration (for future implementation)
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_APP_ID_ANDROID=your_android_app_id
FIREBASE_APP_ID_IOS=your_ios_app_id

# Build Configuration
BUILD_NUMBER=1
VERSION_NAME=1.0.0